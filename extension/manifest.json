{
  "manifest_version": 2,

  "name": "sumeX",
  "description": "sumeX summarizes paragraphs of texts, saving you time from reading large wall of texts.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "<all_urls>","contextMenus"],

  "background": {
    "scripts": ["popup.js"]
  }
}
